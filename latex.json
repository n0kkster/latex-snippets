{
	"Align*": {
		"prefix": "al",
		"body": [
			"\\begin{align*}",
			"\t$0",
			"\\end{align*}"
		]
	},
	"bold": {
		"prefix": "bold",
		"body": [
			"\\textbf{${1:${TM_SELECTED_TEXT}}} $0"
		]
	},
	"italic": {
		"prefix": "italic",
		"body": [
			"\\textit{${1:${TM_SELECTED_TEXT}}} $0"
		]
	},
	"underbrace": {
		"prefix": "underbrace",
		"body": [
			"\\underbrace{${1:${TM_SELECTED_TEXT}}}_{$2} $0"
		]
	},
	"overbrace": {
		"prefix": "overbrace",
		"body": [
			"\\overbrace{${1:${TM_SELECTED_TEXT}}}_{$2} $0"
		]
	},
	"underset": {
		"prefix": "underset",
		"body": [
			"\\underset{$1}{${2:${TM_SELECTED_TEXT}}} $0"
		]
	},
	"overset": {
		"prefix": "overset",
		"body": [
			"\\overset{$1}{${2:${TM_SELECTED_TEXT}}} $0"
		]
	},
	"Environment": {
		"prefix": "beg",
		"body": [
			"\\begin{${1:name}}",
			"\t$0",
			"\\end{${1:name}}"
		]
	},
	"Text": {
		"prefix": "txt",
		"body": [
			"\\text{$0}"
		]
	},
	"pmatrix": {
		"prefix": "pm",
		"body": [
			"\\begin{pmatrix}",
			"$1",
			"\\end{pmatrix}",
			"$0"
		]
	},
	"undersetinline": {
		"prefix": "_",
		"body": [
			"_{$1}$0"
		]
	},
	"parenthesis1": {
		"prefix": "(",
		"body": [
			"\\left($1\\right"
		]
	},
	"parenthesis2": {
		"prefix": "[",
		"body": [
			"\\left[$1\\right"
		]
	},
	"parenthesis3": {
		"prefix": "{",
		"body": [
			"\\left{$1\\right"
		]
	},
	"average": {
		"prefix": "avg",
		"body": [
			"\\langle $1 \\rangle$0"
		]
	},
	"hbar": {
		"prefix": "hb",
		"body": [
			"\\hbar"
		]
	},
	"section": {
		"prefix": "sec",
		"body": [
			"\\section{$1}",
			"$0"
		]
	},
	"subsection": {
		"prefix": "ssec",
		"body": [
			"\\subsection{$1}",
			"$0"
		]
	},
	"operatorname": {
		"prefix": "op",
		"body": [
			"\\operatorname{$1} $0"
		]
	},
	"mathrm": {
		"prefix": "rm",
		"body": [
			"\\mathrm{$1} $0"
		]
	},

	"begin/end equation": {
		"prefix": [
			"eq"
		],
		"body": [
			"\\begin{equation} \\label{eq:$1}",
			"\t$0",
			"\\end{equation}"
		]
	},
	"begin/end void equation": {
		"prefix": [
			"eqv"
		],
		"body": [
			"\\begin{equation*} \\label{eq:$1}",
			"\t$0",
			"\\end{equation*}"
		]
	},
	"begin/end aligned": {
		"prefix": [
			"ali"
		],
		"body": [
			"\\begin{aligned}",
			"$0",
			"\\end{aligned}"
		]
	},
	"begin/end gather": {
		"prefix": [
			"gat"
		],
		"body": [
			"\\begin{gather}",
			"$0",
			"\\end{gather}"
		]
	},
	"begin/end gather*": {
		"prefix": [
			"gatv"
		],
		"body": [
			"\\begin{gather*}",
			"$0",
			"\\end{gather*}"
		]
	},
	"begin/end cases": {
		"prefix": [
			"cas"
		],
		"body": [
			"\\begin{cases}",
			"$0",
			"\\end{cases}"
		]
	},
	"begin/end cases*": {
		"prefix": [
			"casv"
		],
		"body": [
			"\\begin{cases*}",
			"$0",
			"\\end{cases*}"
		]
	},
	"begin/end enumerate": {
		"prefix": [
			"enu",
			"enm"
		],
		"body": [
			"\\begin{enumerate}",
			"\t\\item $0",
			"\\end{enumerate}"
		]
	},
	"begin/end enumerate*": {
		"prefix": [
			"enuv",
			"enmv"
		],
		"body": [
			"\\begin{enumerate*}",
			"\t\\item $0",
			"\\end{enumerate*}"
		]
	},
	"begin/end itemize": {
		"prefix": [
			"itm"
		],
		"body": [
			"\\begin{itemize}",
			"\t\\item $0",
			"\\end{itemize}"
		]
	},
	"begin/end theorem": {
		"prefix": [
			"thm"
		],
		"body": [
			"\\begin{theorem}",
			"$0",
			"\\end{theorem}"
		]
	},
	"begin/end theorem* ": {
		"prefix": [
			"thmv"
		],
		"body": [
			"\\begin{theorem*}",
			"$0",
			"\\end{theorem*}"
		]
	},
	"begin/end corollary": {
		"prefix": [
			"cor"
		],
		"body": [
			"\\begin{corollary}",
			"$0",
			"\\end{corollary}"
		]
	},
	"begin/end corollary* ": {
		"prefix": [
			"corv"
		],
		"body": [
			"\\begin{corollary*}",
			"$0",
			"\\end{corollary*}"
		]
	},
	"begin/end example": {
		"prefix": [
			"ex"
		],
		"body": [
			"\\begin{example}",
			"$0",
			"\\fin\\end{example}"
		]
	},
	"begin/end example* ": {
		"prefix": [
			"exv"
		],
		"body": [
			"\\begin{example*}",
			"$0",
			"\\fin\\end{example*}"
		]
	},
	"begin/end remark": {
		"prefix": [
			"rem"
		],
		"body": [
			"\\begin{remark}",
			"$0",
			"\\fin\\end{remark}"
		]
	},
	"begin/end remark* ": {
		"prefix": [
			"remv"
		],
		"body": [
			"\\begin{remark*}",
			"$0",
			"\\fin\\end{remark*}"
		]
	},
	"begin/end definition": {
		"prefix": [
			"dfn"
		],
		"body": [
			"\\begin{definition}",
			"$0",
			"\\end{definition}"
		]
	},
	"begin/end definition*": {
		"prefix": [
			"dfnv"
		],
		"body": [
			"\\begin{definition*}",
			"$0",
			"\\end{definition*}"
		]
	},
	"begin/end prf": {
		"prefix": [
			"prf"
		],
		"body": [
			"\\begin{proof}",
			"$0",
			"\\end{proof}"
		]
	},
	"listing": {
		"prefix": [
			"list"
		],
		"body": [
			"\\begin{center}",
			"\t\\captionsetup{justification=centering, labelsep=endash, singlelinecheck=on}",
			"\t\\begin{lstlisting}[label=lst:$1, caption=$2]",
			"$3",
			"\t\\end{lstlisting}",
			"\\end{center}"
		]
	},
	"gostlisting": {
		"prefix": [
			"glist"
		],
		"body": [
			"\\begin{listing}[$1]{lst:$2}{C++}",
			"$3",
			"\\end{listing}"
		]
	}
}
